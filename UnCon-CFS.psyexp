    <Routine name="flash_init">
      <CodeComponent name="def_set_colors">
        <Param name="Begin Experiment" updates="constant" val="# Colors used during the experiment.&amp;#10;# Color variables end with _c.&amp;#10;def set_colors():&amp;#10;    global screen_c # screen color&amp;#10;    global frame_c # frame color&amp;#10;    global cross_c  # cross color&amp;#10;    screen_c = [.002, .002, .002]&amp;#10;    frame_c = [-1,-1,-1]&amp;#10;    cross_c = 'black'" valType="extendedCode"/>
        <Param name="Begin JS Experiment" updates="constant" val="" valType="extendedCode"/>
