    <Routine name="flash_init">
      <CodeComponent name="def_set_colors">
        <Param name="Begin Experiment" updates="constant" val="# set_colors()&amp;#10;# Colors used during the experiment.&amp;#10;# Color variables end with _c.&amp;#10;def set_colors():&amp;#10;    global sc_c # screen color&amp;#10;    global f_c # frame color&amp;#10;    global f2_c # frame 2 color&amp;#10;    global sq_c # square color&amp;#10;    global sq_f_c # square color&amp;#10;    global t_c  # text color&amp;#10;    global c_c  # circle color    &amp;#10;    # 1 = white&amp;#10;    # -0.06 = grey 80&amp;#10;    # -0.37 = grey 120&amp;#10;    # -1 = black&amp;#10;    sc_c = [.002, .002, .002]&amp;#10;    f_c = [-1,-1,-1]&amp;#10;    f2_c = [1,1,1]&amp;#10;    sq_c = [.002, .002, .002]&amp;#10;    sq_f_c = [.002, .002, .002]&amp;#10;    t_c = [1,1,1]&amp;#10;    c_c = 'red'" valType="extendedCode"/>
